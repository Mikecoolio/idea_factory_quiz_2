<%= render "shared/header" %>
<%= render "shared/navbar" %>
<div>
    <%= form_with url: session_path do |f| %>
        <%= f.label :email %>
        <%= f.text_area :email %>
        <br>
        <%= f.label :password %>
        <%= f.text_area :password %>
        <br>
        <%= f.submit %>
    <% end %>
</div>